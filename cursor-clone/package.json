{
  "name": "cursor-ai-assistant",
  "displayName": "Cursor AI Assistant",
  "description": "AI-powered coding assistant similar to Cursor",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cursor-ai-assistant.openChat",
        "title": "Open AI Chat",
        "category": "Cursor AI"
      },
      {
        "command": "cursor-ai-assistant.explainCode",
        "title": "Explain Selected Code",
        "category": "Cursor AI"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "cursorAIChat",
          "name": "AI Assistant",
          "when": "true"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "command": "cursor-ai-assistant.explainCode",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "cursor-ai-assistant.openChat",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}